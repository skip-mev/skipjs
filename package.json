{
  "name": "@skip-mev/skipjs",
  "version": "1.0.1",
  "description": "",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skip-mev/skipjs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skip-mev/skipjs/issues"
  },
  "homepage": "https://github.com/skip-mev/skipjs#readme",
  "dependencies": {
    "@cosmjs/amino": "^0.29.0",
    "@cosmjs/crypto": "^0.29.0",
    "@cosmjs/proto-signing": "^0.29.0",
    "cosmjs-types": "^0.5.1",
    "node-fetch-commonjs": "^3.2.4"
  }
}
